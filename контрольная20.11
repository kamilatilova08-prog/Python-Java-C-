# Функция решает задачу минимального покрытия множества
def set_cover(universe, subsets):
    covered = set()      # Уже покрытые элементы
    result = []         # Набор выбранных подмножеств
    
    while len(covered) < len(universe):
        best_subset = None   # Текущее оптимальное подмножество
        max_new_elements = 0 # Количество новых элементов текущего оптимального подмножества
        
        for s in subsets:
            uncovered_in_s = s - covered  # Новые элементы, покрываемые данным подмножеством
            num_new_elements = len(uncovered_in_s)
            
            if num_new_elements > max_new_elements:
                best_subset = s         # Обновляем оптимальный выбор
                max_new_elements = num_new_elements
        
        if best_subset is None or max_new_elements == 0:
            break                      # Если больше ничего добавить нельзя
        
        result.append(best_subset)    # Добавляем найденное подмножество
        covered.update(best_subset)   # Обновляем покрытие
    
    return result


# Ручной ввод данных
universe_input = input("Введите элементы универсального множества через пробел:\n")
subsets_input = []
while True:
    subset_str = input(f"Введите следующее подмножество через пробел (или пустую строку для завершения):\n")
    if not subset_str.strip():
        break
    subsets_input.append(set(subset_str.split()))

# Преобразуем строки в наборы
universe = set(universe_input.split())
subsets = list(map(set, subsets_input))

# Получаем решение
solution = set_cover(universe, subsets)
print("\nРешение:")
for i, subset in enumerate(solution):
    print(f"{i+1}. {subset}")

