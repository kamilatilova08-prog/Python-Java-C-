import java.util.Stack;

public class Geeks 
{
    public static void main(String[] args) 
    {
        // Создаём новый стек
        Stack<Integer> s = new Stack<>();

        // Добавляем элементы в стек
        s.push(1);
        s.push(2);
        s.push(3);
        s.push(4);

        // Метод pop() в Java для работы со стеком  удаляет верхний элемент из стека и возвращает его.
        while(!s.isEmpty()) {
            System.out.println(s.pop());
        }
    }
}
